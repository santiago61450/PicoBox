gravity = 0.3
friction = 0.98
bounce = 0.9

wind_x = -0.1
wind_y = -0.4

init_wind_x = wind_x
init_wind_y = wind_y

wind_max_x = 4
wind_max_y = 2

wind_hit_chance = 0.1

